use ast::Node;

grammar;

pub InputElementDiv: Node = {
  Whitespace,
  LineTerminator,
  Comment,
};

Whitespace: Node = {
  r"\x09|\x0B|\x0C|\xFEFF|\p{Space_Separator}" => Node::Whitespace(<>.to_string())
};

LineTerminator: Node = {
   r"\u000A|\u000D|\u2028|\u2029" => Node::Whitespace(<>.to_string())
};

Comment: Node = {
  r"//[^\u000A\u000D\u2028\u2029]*" => Node::Comment(false, <>.to_string())
};
