grammar;

pub InputElementDiv: String = {
  Whitespace,
  LineTerminator,
  Comment,
};

Whitespace: String = {
  r"\x09|\x0B|\x0C|\xFEFF|\p{Space_Separator}" => <>.to_string()
};

LineTerminator: String = {
   r"\u000A|\u000D|\u2028|\u2029" => <>.to_string()
};

Comment: String = {
  r"//[^\u000A\u000D\u2028\u2029]*" => <>.to_string()
};
